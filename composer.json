{
    "name": "skyraptor/filament-blocks-builder",
    "type": "library",
    "description": "This Filament PHP plugin adds a BlockBuilder form component, that allows you to create layouts using Blocks. This can be used as a builder for anything: layout, content, data and more.",
    "keywords": [
        "laravel",
        "filament",
        "blocks",
        "builder",
        "wysiwyg",
        "layout",
        "content",
        "data"
    ],
    "require": {
        "php": "^8.3",
        "cweagans/composer-patches": "^1.7.0",
        "filament/forms": "^3.2"
    },
    "extra": {
        "patches": {
            "filament/forms": {
                "Fix oldFormState containing new values.": "https://patch-diff.githubusercontent.com/raw/filamentphp/filament/pull/13973.diff"
            }
        }
    }
}
